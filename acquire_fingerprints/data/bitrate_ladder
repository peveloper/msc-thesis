plot_dir = "plots/"
filename = substr(file, 0, strstrt(file, "_") - 1)
set xlabel "Bitrate (bytes/s)"
set ylabel "Throughputs"
set title sprintf("Bitrate Ladder ID: %s", filename)
set term png
set output sprintf("%s%s.png", plot_dir, filename)
plot for [file in files] file.'.dat.bl' with lines title file[strstrt(file, "_") + 1:strlen(file)]
